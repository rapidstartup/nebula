# Wallet for deploying contracts (testnet only - never use real funds wallet)
DEPLOYER_PRIVATE_KEY=0x_your_private_key_here

# RPC URLs (get from Alchemy dashboard)
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/your_api_key
POLYGON_AMOY_RPC_URL=https://polygon-amoy.g.alchemy.com/v2/your_api_key

# API Keys
ETHERSCAN_API_KEY=your_etherscan_key_here
VITE_ALCHEMY_API_KEY=your_alchemy_key

# WalletConnect (get from https://cloud.walletconnect.com)
# Required for WalletConnect wallet connections
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Optional: OpenRouter for AI features
OPENROUTER_API_KEY=your_openrouter_key